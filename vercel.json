{
  "version": 2,
  "name": "royal-videography",
  "builds": [
    {
      "src": "dist/public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/dist/public/index.html"
    }
  ],
  "github": {
    "silent": true
  },
  "env": {
    "VITE_STRIPE_PUBLIC_KEY": "@vite_stripe_public_key"
  }
}